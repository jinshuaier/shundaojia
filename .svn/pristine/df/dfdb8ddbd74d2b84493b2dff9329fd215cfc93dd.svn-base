typedef void(^success)(void);
typedef void(^failure)(void);
@interface TCShowView : UIView
@property (nonatomic, copy) success success;
@property (nonatomic, copy) failure failure;
+ (id)showView:(success)success andfailure:(failure)failure;
@end
